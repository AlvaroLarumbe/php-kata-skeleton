{
    "name": "alvarolarumbe/php-kata-skeleton",
    "description": "PHP Kata skeleton",
    "keywords": ["kata"],
    "require": {
        "php": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "Kata\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KataTest\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "phpunit --configuration phpunit.xml"
        ]
    },
